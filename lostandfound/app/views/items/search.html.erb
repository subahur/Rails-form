<h1> Welcome to the search page </h1>

<%= form_tag(action: "search", method: "post") do %>
  <%= label_tag(:search, "Search for:") %>
  <%= text_field_tag(:search) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
    <% @items.each do |item| %>
  <li><%= link_to item.title, item_path(item)  %></li>
  <% end %>
</ul>
